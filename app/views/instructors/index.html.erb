 <div class="container">
    <h5>Instructors</h5>
    <p>These are all our instructors</p>
    
<table class="centered">
        <thead>
          <tr>
              <th>FirstName</th>
              <th>LastName</th>
              <th>Education Level</th>
              <th>Delete</th>
              <th>Edit</th>
          </tr>
        </thead>
        <%@instructors.each do |instructor|%>
            <tbody>
                <tr>
                    <td><%=instrucor.firstname%></td>
                    <td><%=instructor.lastname%></td>
                    <td><%=instructor.educationlevel%></td>
                    <th><%= link_to "Show ", instructor_path(instrucor) %> </th>
                    <th><%= link_to "Edit ", edit_instructor_path(instrucor) %> </th>
                    <th><%= link_to "Destroy ", instructor_path(instrucor), method: :delete %> </th>
                </tr>
        <%end%>
          
        </tbody>
      </table>
    </div>
    <a href="/instructors/new">Add Instructors</a>